{
  "name": "@kyoji2/raindrop-cli",
  "version": "0.1.2",
  "description": "AI-native CLI for Raindrop.io",
  "author": "kyoji2",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kyoji2/raindrop-cli.git"
  },
  "bugs": {
    "url": "https://github.com/kyoji2/raindrop-cli/issues"
  },
  "homepage": "https://github.com/kyoji2/raindrop-cli#readme",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "module": "src/index.ts",
  "type": "module",
  "exports": {
    ".": "./src/api/index.ts",
    "./api": "./src/api/index.ts",
    "./utils": "./src/utils/index.ts"
  },
  "bin": {
    "raindrop": "dist/index.js"
  },
  "scripts": {
    "start": "bun run src/index.ts",
    "dev": "bun --watch src/index.ts",
    "test": "bun test",
    "lint": "biome check ./src ./tests",
    "lint:fix": "biome check --write ./src ./tests",
    "format": "biome format --write ./src ./tests",
    "build": "bun build src/index.ts --outdir dist --target bun && chmod +x dist/index.js",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@toon-format/toon": "^2.1.0",
    "commander": "^14.0.2",
    "ora": "^9.0.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "keywords": [
    "raindrop",
    "bookmarks",
    "cli",
    "ai-native"
  ],
  "license": "MIT"
}
